{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "jamesgifford/laravel-architecture",
    "description": "Support for my preferred Laravel architectural practices.",
    "keywords": [
        "laravel",
        "architecture"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JamesGifford\\LaravelArchitecture\\": "src/"
        }
    },
    "authors": [
        {
            "name": "James Gifford"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
    	"php": "^8.5",
    	"laravel/framework": "^12.0"
    },
    "extra": {
    	"laravel": {
      		"providers": [
        		"JamesGifford\\LaravelArchitecture\\LaravelArchitectureServiceProvider"
      		]
    	}
    }
}
